<h1>Riders location</h1>

<div id="map"></div>
<script src='https://maps.googleapis.com/maps/api/js?key=AIzaSyB8qYzU7DlFDA2gPWyEa-tnFsIpNdHqgw8&callback=initMap'
async defer></script>

<script>
  function initMap() {
    var myLatLng = {lat: 40.00, lng: -105.35};

    // Create a map object and specify the DOM element
    // for display.
    var map = new google.maps.Map(document.getElementById('map'), {
      center: myLatLng,
      zoom: 12
    });

    // Create a marker and set its position.
    var marker = new google.maps.Marker({
      map: map,
      position: myLatLng,
      title: 'Dustin Green'
    });
  }
</script>
